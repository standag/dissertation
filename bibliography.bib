author = {Bissantz, Caterina and Folkers, Gerd and Rognan, Didier},

issn = {0022-2623},
journal = {Journal of medicinal chemistry},
month = dec,
number = {25},
pages = {4759--4767},
publisher = {American Chemical Society},
title = {{Protein-Based Virtual Screening of Chemical Databases. 1. Evaluation of Different Docking/Scoring Combinations}},
url = {http://dx.doi.org/10.1021/jm001044l},
