keywords = {Allergens,Allergens: chemistry,Amino Acid Sequence,Betula,Betula: chemistry,Models,Molecular,Molecular Sequence Data,Plant Proteins,Plant Proteins: chemistry,Pollen,Pollen: chemistry,Protein Structure,Sequence Alignment,Tertiary},
