file = {:C$\backslash$:/Users/Lukas/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hauck et al. - 2013 - Discovery of two classes of potent glycomimetic inhibitors of Pseudomonas aeruginosa LecB with distinct binding mo.pdf:pdf},
issn = {1554-8937},
journal = {ACS chemical biology},
keywords = {Binding Sites,Binding, Competitive,Biomimetics,Biomimetics: classification,Cell Adhesion,Crystallography, X-Ray,Drug Discovery,Lectins,Lectins: antagonists {\&} inhibitors,Ligands,Mannose,Mannose: chemistry,Mannose: pharmacology,Molecular Structure,Protein Binding,Pseudomonas aeruginosa},
month = {aug},
number = {8},
pages = {1775--84},
pmid = {23719508},
title = {{Discovery of two classes of potent glycomimetic inhibitors of Pseudomonas aeruginosa LecB with distinct binding modes.}},
