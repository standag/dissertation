pages = {134--9},
pmid = {19171968},
title = {{On vital aid: the why, what and how of validation.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2631629{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {65},
year = {2009}
}

